ENDIF
IF P > 40
    LET P 40
ENDIF
IF KEY FIRE2
    MENU 3
    REDRAW
ENDIF
IF B = 255
    IF C = 255
        LET B Y
        LET C X
    ENDIF
ENDIF
ADD 1 TO SETTINGA
IF SETTINGA > 2
    LET SETTINGA 0
ENDIF
DETECTOBJ
IF OBJ > 3
    IF OBJ <= 10
        BEEP 90
    ENDIF
ENDIF
IF OBJ = 255
ELSE
    GET OBJ
    SCORE 10
    BEEP 90
ENDIF
; warning - ENDIF without IF?
ENDIF
TABLEFALL
IF Y <= 12
    LET Y 126
    SCREENUP
    LET D 0
ENDIF
IF Y > 132
    LET Y 13
    SCREENDOWN
    LET D 0
ENDIF
IF KEY RIGHT
    IF X > 230
        SCREENRIGHT
        LET D 0
        LET X = 16
        EXIT
    ELSE
        IF CANGORIGHT
            LET IMAGE = 0
            ANIMATE
            IF SETTINGA = 0
                BEEP 5
            ENDIF
            SPRITERIGHT
        ENDIF
    ENDIF
ENDIF
IF KEY LEFT
    IF X <= 8
        SCREENLEFT
        LET D 0
        LET X = 228
        EXIT
    ELSE
        IF CANGOLEFT
            LET IMAGE = 1
            ANIMATE
            IF SETTINGA = 0
                BEEP 5
            ENDIF
            SPRITELEFT
        ENDIF
    ENDIF
ENDIF
IF KEY FIRE
    TABLEJUMP
    IF CANGODOWN
    ELSE
        BEEP 60
    ENDIF
ENDIF
IF DEADLY
    KILL
ENDIF


